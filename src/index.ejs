<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Stage Pass</title>
  <script src="https://cdn.jsdelivr.net/npm/regenerator-runtime@0.13.7/runtime.min.js"></script>
  <!-- <meta http-equiv="Content-Security-Policy" content="default-src 'self' https: localhost:*; script-src 'unsafe-inline' 'unsafe-eval' https: localhost:*; connect-src https: localhost:* ws://localhost:*; style-src 'unsafe-inline' https:; object-src 'none';"> -->
  <meta name="importmap-type" content="systemjs-importmap">
  <meta name="theme-color" content="#f69435">
  <meta name="description" content="WebApp to help buy tickets of all kinds of events">


  <% if (isLocal) { %>
    <link rel="manifest" href="../public/manifest.json">
    <link rel="apple-touch-icon" href="../public/icon-180x180.png">
    <link rel="icon" href="../public/favicon.ico" type="image/x-icon">
  <% } else { %>
    <link rel="manifest" href="./public/manifest.json">
    <link rel="apple-touch-icon" href="./public/icon-180x180.png">
    <link rel="icon" href="./public/favicon.ico" type="image/x-icon">
  <% } %>


  <script type="systemjs-importmap">
    {
      "imports": {
        "single-spa": "https://cdn.jsdelivr.net/npm/single-spa@5.9.0/lib/system/single-spa.min.js",
        "react": "https://cdn.jsdelivr.net/npm/react@17.0.2/umd/react.production.min.js",
        "react-dom": "https://cdn.jsdelivr.net/npm/react-dom@17.0.2/umd/react-dom.production.min.js",
        "zustand": "https://unpkg.com/zustand@3.6.9/system/index.js",
        "zustand/middleware": "https://unpkg.com/zustand@3.6.9/system/middleware.js"
      }
    }
  </script>

  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link
    href="https://fonts.googleapis.com/css2?family=Raleway+Dots&family=Raleway:wght@400;500;600;700&family=Varela+Round&display=swap"
    rel="stylesheet">


  <% if (isLocal) { %>
    <script type="systemjs-importmap">
  {
    "imports": {
      "@stagepass/root-config": "//localhost:9000/stagepass-root-config.js",
      "@stagepass/app-events": "//localhost:8080/stagepass-app-events.js",
      "@stagepass/osiris-ui": "//localhost:8081/stagepass-osiris-ui.js",
      "@stagepass/util-state": "//localhost:8082/stagepass-util-state.js"
    }
  }
  </script>

  <% } else { %>
    <script type="systemjs-importmap" src="/config/import-map.json"></script>
  <% } %>

  <script
    src="https://cdn.jsdelivr.net/npm/import-map-overrides@2.2.0/dist/import-map-overrides.js"></script>
  <% if (isLocal) { %>
    <script src="https://cdn.jsdelivr.net/npm/systemjs@6.8.3/dist/system.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/systemjs@6.8.3/dist/extras/amd.js"></script>
  <% } else { %>
      <script src="https://cdn.jsdelivr.net/npm/systemjs@6.8.3/dist/system.min.js"></script>
      <script src="https://cdn.jsdelivr.net/npm/systemjs@6.8.3/dist/extras/amd.min.js"></script>
  <% } %>

</head>

<body>
  <noscript>You need to enable JavaScript to run this app.</noscript>

  <script>
    System.import('@stagepass/root-config')

    if ('serviceWorker' in navigator) {
      window.addEventListener('load', () => {
        navigator.serviceWorker.register('./service-worker.js').then(registration => {
          console.log('SW registered: ', registration)
        }).catch(registrationError => {
          console.log('SW registration failed: ', registrationError)
        })
      })
    }

  </script>

  <import-map-overrides-full show-when-local-storage="devtools" dev-libs>
  </import-map-overrides-full>
</body>

</html>